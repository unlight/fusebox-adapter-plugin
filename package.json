{
  "name": "fusebox-adapter-plugin",
  "version": "1.0.0",
  "description": "Adapt gulp plugins, browserify transforms and streams to work with fuse-box",
  "main": "lib",
  "typings": "lib",
  "scripts": {
    "clean": "rm -rf .fusebox lib",
    "compile": "tsc",
    "compile:w": "tsc -w",
    "test": "npm run clean && npm run compile && npm run test:r",
    "test:r": "ava lib/*.spec.js --verbose --serial --no-cache",
    "test:w": "ava lib/*.spec.js --verbose --serial --watch",
    "dev": "concurrently -p \"{name}:\" -n \"w,t\" \"npm run compile:w\" \"npm run test:w\""
  },
  "devDependencies": {
    "@types/node": "~7.0.5",
    "@types/through2": "~2.0.32",
    "@types/vinyl": "~2.0.0",
    "ava": "~0.18.1",
    "concurrently": "~3.4.0",
    "envify": "~4.0.0",
    "gulp-debug": "~3.1.0",
    "gulp-eslint": "~3.0.1",
    "gulp-inject-string": "~1.1.0",
    "gulp-json5": "~0.0.2",
    "gulp-load-plugins": "~1.5.0",
    "gulp-markdown": "~1.2.0",
    "gulp-replace": "~0.5.4",
    "gulp-size": "~2.1.0",
    "is-stream": "1.1.0",
    "through2": "~2.0.3",
    "tslib": "~1.6.0",
    "typescript": "~2.2.1"
  },
  "peerDependencies": {
    "fuse-box": "^1.3.24"
  },
  "dependencies": {
    "pumpify": "1.3.5",
    "stream-to-string": "1.1.0",
    "toSrc": "0.1.4",
    "vinyl": "2.0.1"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "gulp",
    "fuse-box",
    "browserify"
  ],
  "license": "XNet"
}
